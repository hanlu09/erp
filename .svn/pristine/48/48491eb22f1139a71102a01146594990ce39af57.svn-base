package cn.itcast.erp.entity;
/**
 * 报表封装专用实体类(用于封装报表查询结果 )
 * @author Administrator
 *
 */
public class Report {
	
	private String name;
	private double money;
	
	public Report(String name,double money){
		this.name=name;
		this.money=money;		
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getMoney() {
		return money;
	}
	public void setMoney(double money) {
		this.money = money;
	}
	
}
